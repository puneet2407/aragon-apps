{
  "name": "@1hive/apps-time-lock",
  "version": "2.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:template",
    "start:template": "aragon run --template Template --template-init @ARAGON_ENS --template-args false --files dist  --block-time 1",
    "start:template:oracle": "aragon run --template Template --template-init @ARAGON_ENS --template-args true --files dist --block-time 1",
    "start:http": "npm run start:http:template",
    "start:http:template": "aragon run --template Template --template-init @ARAGON_ENS --template-args false --http localhost:8001 --http-served-from ./dist --block-time 1",
    "build": "cd app && npm run build && cd ..",
    "prepare": "cd app && npm install && cd ..",
    "start:app": "cd app && npm start && cd ..",
    "prepublishOnly": "aragon contracts compile",
    "publish:patch": "aragon apm publish patch --files dist",
    "publish:minor": "aragon apm publish minor --files dist",
    "publish:major": "aragon apm publish major --files dist",
    "deploy:staging": "npm run publish:major -- --environment staging",
    "versions": "aragon apm versions",
    "lint": "eslint . & solium --dir ./contracts",
    "lint:fix": "eslint . --fix & solium --dir ./contracts --fix",
    "coverage": "cross-env SOLIDITY_COVERAGE=true npm run ganache-cli:test",
    "coveralls": "cat ./coverage/lcov.info | npx coveralls",
    "ganache-cli:test": "sh ./node_modules/@aragon/test-helpers/ganache-cli.sh",
    "test": "cross-env TRUFFLE_TEST=true npm run ganache-cli:test",
    "set-oracle": "./scripts/setTokenOracle.sh development",
    "set-oracle:rinkeby": "./scripts/setTokenOracle.sh rinkeby",
    "set-oracle:mainnet": "./scripts/setTokenOracle.sh mainnet"
  },
  "files": [
    "/arapp.json",
    "/build",
    "/contracts",
    "/test"
  ],
  "keywords": [],
  "description": "",
  "devDependencies": {
    "@1hive/oracle-token-balance": "^2.0.0",
    "@aragon/apps-shared-minime": "1.0.0",
    "@aragon/apps-token-manager": "^2.1.0",
    "@aragon/apps-vault": "^4.1.0",
    "@aragon/apps-voting": "^2.1.0",
    "@aragon/cli": "^6.3.3",
    "@aragon/test-helpers": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "coveralls": "3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "eth-ens-namehash": "^2.0.8",
    "ethlint": "^1.2.3",
    "ganache-cli": "^6.0.0",
    "prettier": "^1.17.1",
    "solidity-coverage": "^0.6.2",
    "truffle": "^5.0.1"
  },
  "dependencies": {
    "@aragon/os": "^4.0.1"
  }
}
